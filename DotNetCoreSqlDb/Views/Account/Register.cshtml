@model RegisterViewModel

<h2>Register</h2>

<form method="post">
    <div>
        <label>Email</label>
        <input asp-for="Email" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Password" />
    </div>
    <div>
        <label>Confirm Password</label>
        <input asp-for="ConfirmPassword" />
    </div>
    <div>
        <button type="submit">Register</button>
    </div>
</form>
